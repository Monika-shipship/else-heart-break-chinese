
# Reserved for heavy calculations.

number SUM(array a)
    var sum = 0
    loop x in a
        sum += x
    end
    var hugin = Connect("Hugin")
    hugin.Print("Sum calculation result: " + sum)
    return sum
end

number PRODUCT(array a)
    var product = 0
    loop x in a
        product *= x
    end
    var hugin = Connect("Hugin")
    hugin.Print("Product calculation result: " + product)
    return product
end